{
    "listeners":{
       "*:8814":{
          "pass":"routes"
       }
    },
    "routes":[
       {
          "match":{
             "uri":"/static/*"
          },
          "action":{
             "share":"/opt/app/frontend$uri"
          }
       },
       {
          "match":{
             "uri":"/media/*"
          },
          "action":{
             "share":"/opt/app$uri"
          }
       },
       {
          "action":{
             "pass":"applications/django"
          }
       }
    ],
    "applications":{
       "django":{
          "type":"python",
          "processes":4,
          "threads":4,
          "path":"/opt/app",
          "module":"club.wsgi"
       }
    }
 }